{
    "requestVerifyCode":
    {
        "creator":"createHttpAction",
        "property":
        {
            "url":"/userSms/sendByRegister",
            "param":
            {
                "mobilePhone":"$userInfo.userName",
                "api_key":"verify"
            }
        },
        "complete":"",
        "failed":""
    },
    "sendRegister":
    {
        "creator":"createHttpAction",
        "property":
        {
            "url":"/user/register",
            "param":
            {
                "loginUserId":"$userInfo.userId",
                "mobilePhone":"$userInfo.mobilePhone",
                "password":"$user.userPassword",
                "idenCode":"$globalData.verifyCode",
                "deviceType":"$globalData.deviceType",
                "deviceId":"globalData.deviceId",
                "api_key":"verify"
            }
        },
        "complete":"registerComplete",
        "failed":"registerFailed"
    },
    "registerComplete":
    {
        "creator":"createCustomAction",
        "property":
        {
        },
        "complete":"",
        "failed":""
    },
    "registerFailed":
    {
        "creator":"createCustomAction",
        "property":
        {
        },
        "complete":"",
        "failed":""
    }
}