{
	"rootView":
	{
		"creator":"createView",
		"property":
		{
			"frame":"{0,0,320,568}",
			"bgcolor":"{255,255,255,255}"
		}
	},
    "logoView":
	{
		"creator":"createImageView",
		"property":
		{
			"frame":"{0,0,200,200}",
			"bgcolor":"{255,255,255,255}"
			"image":"app_log_200X200.png"
		}
	},
	"userNameText":
	{
		"creator":"createTextField",
		"property":
		{
			"frame":"{10,230,100,30}",
			"bgcolor":"{255,255,255,255}"
		}
	},
	"userPasswordText":
	{
		"creator":"createTextField",
		"property":
		{
			"frame":"{10,270,100,30}",
			"bgcolor":"{255,255,255,255}"
		}
	},
	"register":
	{
		"creator":"createButton",
		"property":
		{

		}
	},
	"userInfo":
	{
		"creator":"createData",
		"property":
		{
			"userName":"",
			"userPassword":""
		}
	},
	"nameBinder":
	{
		"creator":"viewDataBinder",
		"property":
		{
			"srcObj":"$userInfo.userName",
			"destObj":"$userNameText"
		}
	},
	"passwordBinder":
	{
		"creator":"viewDataBinder",
		"property":
		{
			"srcObj":"$userInfo.userPassword",
			"destObj":"$userPasswordText"
		}
	},
	"requestVerifyCode":
	{
		"creator":"createHttpAction",
		"property":
		{
			"url":"/userSms/sendByRegister",
			"param":
			{
				"mobilePhone":"$userInfo.userName",
				"api_key":"verify"
			}
		}
		"complete":"",
		"failed":""
	},
	"sendRegister":
	{
		"creator":"createHttpAction",
		"property":
		{
			"url":"/user/register",
			"param":
			{
				"loginUserId":"$userInfo.userId",
				"mobilePhone":"$userInfo.userName",
				"password":"$userInfo.userPassword",
				"idenCode":"$userInfo.verifuCode",
				"deviceType":"$userInfo.deviceType",
				"deviceId":"$userInfo.deviceId",
				"api_key":"verify"
			}
		}
		"complete":"",
		"failed":""
	},
	"registerComplete":
	{
		"creator":"createCustomAction",
		"property":
		{
		}
	},
	"registerFailed":
	{
		"creator":"createCustomAction",
		"property":
		{

		}
	}
}
